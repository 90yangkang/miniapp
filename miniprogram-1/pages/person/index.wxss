  margin: 40rpx;
  width: 70%;
  border-width: thin;
  background-color: aliceblue;
  border-color:  blue;
  display: flex;
  flex-direction: column;
}

.usertitleimgview {
  width:100%;
  height:280rpx;
  color: #aaa;
}