.outcontainer {
  width: 100vw;
  height: 100vh;
  padding: 20rpx;
  background-image: url("data:image/jpg;base64,");
  background-repeat: no-repeat;
  background-size:cover;
}

.usertitle {
  display: flex;
  width: 100%;
  justify-content: space-between;
  flex-direction: row;
}

.usertitle .usertitleimg {
  width: 35%;
  border-width: thin;
  border-color:  blue;
}

.usertitle .usertitleinfo {
  margin: 40rpx;
  width: 70%;
  border-width: thin;
  background-color: aliceblue;
  border-color:  blue;
  display: flex;
  flex-direction: column;
}

.usertitleimgview {
  width:100%;
  height:280rpx;
  color: #aaa;
}

.displayshow {
  display: block;
}

.displayhidden {
  display: none;
}

.personlistitem {
  border: 1rpx solid #BBBBBB;
  background-color: #e7dbca;
  width: 90%;
  margin-top: 25rpx;
  padding: 20rpx;
}

#personimageslist {
  width: 100vw;
  height: 100vh;
  padding: 10rpx;
}

.personimagesitem {
  width: 35%;
  margin: 15rpx;
}

